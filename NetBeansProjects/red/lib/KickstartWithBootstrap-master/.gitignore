# eclipse specific
plugin.xml
/.classpath
.classpath
/.project
.settings
/target-eclipse
org.eclipse.core.resources.prefs
org.eclipse.jdt.groovy.core.prefs

# IDEA specific
.idea
*.iml
*.iws
*.ipr

####################

# Windows specific
Thumbs.db

# Mac specific
.DS_Store

